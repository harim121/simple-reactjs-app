{
  "name": "simple-reactjs-app",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://aditya-sridhar.github.io/simple-reactjs-app",
  "dependencies": {
    "axios": "^0.18.0",
    "react": "^16.4.1",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.4.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4"

  },
  "scripts": {
    "sonar": "sonar-scanner",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest --collectCoverage",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "jest": {
    "collectCoverageFrom": [
    "src/**/*.{js,jsx}",
    "!**/node_modules/**",
    "!build/**"
    ],
    "coverageReporters": [
    "lcov",
    "text"
    ]
    },
    "jest-junit": {
    "output": "test-results/test-report.junit.xml"
    },
  "devDependencies": {
    "gh-pages": "^1.2.0",
    "sonar-scanner":"3.1.0",
    "supertest": "3.0.0",
    "nock": "13.0.4",
    "nyc": "15.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.7.0",
    "jest-junit": "^3.6.0"
  }
}
